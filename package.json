{
  "name": "workbench",
  "version": "1.0.0",
  "description": "Production-ready backend infrastructure with PostgreSQL, Redis, MongoDB, Elasticsearch, and ClickHouse",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "cluster:setup": "node scripts/setup-redis-cluster.js",
    "setup": "npm install && npm run db:generate && npm run db:migrate"
  },
  "dependencies": {
    "@prisma/client": "^6.19.1",
    "@prisma/extension-accelerate": "^3.0.1",
    "bcryptjs": "^3.0.3",
    "colorette": "^2.0.20",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "glob": "^13.0.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "node-cron": "^3.0.3",
    "redis": "^5.10.0",
    "resend": "^6.6.0",
    "source-map-support": "^0.5.21",
    "winston": "^3.19.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "prisma": "^6.19.1"
  },
  "keywords": [
    "workbench",
    "backend",
    "node.js",
    "express",
    "postgresql",
    "redis"
  ],
  "author": "Manish",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
