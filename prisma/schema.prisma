generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model HealthCheck {
  id        String   @id @default(cuid())
  status    String
  timestamp DateTime @default(now())
  uptime    Float?
  memory    Json?
  version   String?
  
  @@map("health_checks")
}
